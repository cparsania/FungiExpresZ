journal = {Database : the journal of biological databases and curation},
pages = {bar062},
pmid = {22434830},
publisher = {Oxford University Press},
title = {{YeastMine--an integrated data warehouse for Saccharomyces cerevisiae data as a multipurpose tool-kit.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/22434830 http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=PMC3308152},
volume = {2012},
year = {2012}
}
