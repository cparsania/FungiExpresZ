title = {{Transcript-level expression analysis of RNA-seq experiments with HISAT, StringTie and Ballgown.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/27560171 http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=PMC5032908},
volume = {11},
year = {2016}
}
